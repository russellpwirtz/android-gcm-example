<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.russellpwirtz.gcmpush"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="16" />

    <application
        android:label="Gcm Push App">

        <!-- The launcher activity -->
        <activity
            android:name="com.russellpwirtz.gcmpush.MainActivity"
            android:label="Gcm Push App" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Activity defined in the library module -->
        <activity android:name="com.russellpwirtz.library.LibraryActivity" />
    </application>
</manifest>
